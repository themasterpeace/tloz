<?xml version="1.0" encoding="UTF-8"?>
    <configuration>
        <system.webServer>
            <handlers>
                <add name="Python FastCGI" 
                     path="*" 
                     verb="*" 
                     modules="FastCgiModule" 
                     scriptProcessor="c:\users\mesquite\appdata\local\programs\python\python38\lib\site-packages (3.0.0)" 
                     resourceType="Unspecified" 
                     requireAccess="Script"/>
            </handlers>
